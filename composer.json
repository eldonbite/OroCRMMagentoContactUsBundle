{
    "name": "oro/crm-magento-embedded-contact-us",
    "description": "OroCRM Package, Contact Us form for Magento",
    "homepage": "https://github.com/orocrm/OroCRMMagentoContactUsBundle.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "OroCRM\\Bundle\\MagentoContactUsBundle": "src/"
        }
    },
    "target-dir": "src/OroCRM/Bundle/MagentoContactUsBundle",
    "repositories": [
       {
            "type": "composer",
            "url": "http://packagist.orocrm.com"
        }
    ],
    "require": {
        "php": ">=5.4.4",
        "oro/crm": "1.3.x-dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
